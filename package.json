{
  "name": "xamarin-v8",
  "version": "1.0.0",
  "description": "V8 Bindings for Xamarin for Android (iOS coming soon, only for local development)",
  "main": "index.js",
  "keywords": [
    "react-native",
    "android",
    "v8"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kudo/react-native-v8/issues"
  },
  "homepage": "https://github.com/Kudo/react-native-v8#readme",
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rm -rf dist; rm -rf build; rm -rf v8/out.v8.*",
    "export_npm_env": "./scripts/export_npm_env.sh",
    "setup": "bash -c '. ./scripts/export_npm_env.sh && ./scripts/setup.sh android'",
    "start": "bash -c '. ./scripts/export_npm_env.sh && ./scripts/start.sh android'"    
  },
  "config": {
    "RN_VERSION": "master",
    "V8": "8.0.426.16"
  },
  "dependencies": {
    "v8-android": "~8.80.1"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "clang-format": "^1.4.0"
  }
}
