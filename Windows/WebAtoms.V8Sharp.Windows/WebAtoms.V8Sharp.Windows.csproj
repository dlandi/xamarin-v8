      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="..\V8Sharp\x64\Debug\V8Sharp.dll">
      <Link>V8Sharp.dll</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="..\V8Sharp\x64\Debug\V8Sharp.pdb">
      <Link>V8Sharp.pdb</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="..\V8Sharp\x64\Debug\v8_libbase.dll">
