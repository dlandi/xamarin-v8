    <Content Include="..\V8Sharp\x64\Debug\icuuc.dll">
      <Link>icuuc.dll</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="..\V8Sharp\x64\Debug\v8.dll">
